<% layout("layouts/boilerplate") %>
<h2>Users</h2>
<hr>
<% for(let user of users){ %> 
    <a href="/admin/users/<%= user._id %>">
        <div class="row">
            <div class="col"><%= user.businessName %></div>
            <div class="col"><%= user.name %></div>
            <div class="col"><%= user.email %></div>
        </div>
    </a>
    <hr>
<% } %> 